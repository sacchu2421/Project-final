@using ShoppingCartMVC.ViewModel
@using ShoppingCartMVC.Models
@model UserViewModel
@{
    ViewBag.Title = "GetAllUserDetails";
}

<html>
    <body>
    <table class="table table-striped table-info"align="center" border="2">
        <thead>
        <tr class="w3-red">
        <th scope="col">User Id</th>
        <th scope="col">User Name</th>
        <th scope="col">Address</th>
        <th scope="col">Phone</th>
        </tr>
        </thead>
        
          
        @foreach (User user in Model.users)
        {   
         <tr>
        <td>@user.UserId</td>      
        <td>@user.UserName</td> 
          <td>@user.Address</td>
          <td>@user.Phone</td>
        </tr>
        
       }
     </table>
     <a href="/Admin/AdminPage/1">Go Back</a>
     
   </body>

   </html>
